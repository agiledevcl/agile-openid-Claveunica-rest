<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>OpenIdConceptual</servlet-name>
  <display-name>OpenIdConceptual</display-name>
  <servlet-class>cl.agile.openid.OpenIdConceptual</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>returnResponse</servlet-name>
  <display-name>returnResponse</display-name>
  <servlet-class>cl.agile.openid.returnResponse</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>showResult</servlet-name>
 	<display-name>showResult</display-name>
 	<description></description>
 	<servlet-class>cl.agile.openid.ShowResult</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>OpenIdConceptual</servlet-name>
  <url-pattern>/OpenIdCU</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
 	<servlet-name>returnResponse</servlet-name>
  <url-pattern>/return</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>showResult</servlet-name>
  	<url-pattern>/showResult</url-pattern>
  </servlet-mapping>
</web-app>
